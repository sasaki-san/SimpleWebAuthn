{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/index.html",
            "dest": "/public/index.html"
        },
        {
            "src": "/styles.css",
            "dest": "/public/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}